{
    "name": "heyday/silverstripe-analytics",
    "description": "Heyday's standard analytics module",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/heyday/silverstripe-analytics",
    "keywords": [
        "silverstripe",
        "analytics",
        "google analytic",
        "google tag manager"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/WPP-Public/akqa-nz-silverstripe-analytics/issues"
    },
    "require": {
        "silverstripe/framework": "^6",
        "composer/installers": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Heyday\\Analytics\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
